@using FluentSignals.Web.Shared.Components

<Router AppAssembly="typeof(ISharedMarker).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="routeData" DefaultLayout="typeof(MainLayout)" />
        <FocusOnNavigate RouteData="routeData" Selector="h2" />
    </Found>
    <NotFound>
        <RedirectAction Url="/NotFound" /> 
    </NotFound>
</Router>
